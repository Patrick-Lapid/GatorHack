{
  "manifest_version": 3,
  "name": "Overlay Extension",
  "version": "1.0",
  "description": "A simple overlay extension",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
